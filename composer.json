{
    "name": "bryaneschultz/vite-manifest-plugin",
    "description": "Twig utility function to handle Vite manifest asset files for Craft CMS.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "craft4",
        "cms",
        "craftcms",
        "craft-plugin",
        "twig",
        "vite",
        "manifest"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan E. Schultz",
            "homepage": "https://github.com/bryaneschultz"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "bryaneschultz\\vitemanifest\\": "src/"
        }
    },
    "extra": {
        "name": "Vite Manifest",
        "handle": "vite-manifest",
        "developer": "Bryan E. Schultz",
        "developerUrl": "https://github.com/bryaneschultz",
        "schemaVersion": "1.0.0",
        "documentationUrl": "https://github.com/bryaneschultz/vite-manifest-plugin/blob/main/CHANGELOG.md",
        "changelogUrl": "https://github.com/bryaneschultz/vite-manifest-plugin/blob/main/CHANGELOG.md",
        "hasCpSettings": false,
        "hasCpSection": false,
        "components": {
            "service": "bryaneschultz\\vitemanifest\\services\\ManifestService"
        },
        "class": "bryaneschultz\\vitemanifest\\ViteManifest"
    }
}
